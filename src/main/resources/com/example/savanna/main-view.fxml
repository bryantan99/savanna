<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.savanna.controller.MainController">
   <children>
      <HBox prefHeight="720.0" prefWidth="1280.0">
         <children>
            <VBox prefHeight="720.0" prefWidth="320.0">
               <children>
                  <Pane prefHeight="687.0" prefWidth="320.0" />
                  <ButtonBar prefHeight="40.0" prefWidth="209.0">
                    <buttons>
                      <Button mnemonicParsing="false" onAction="#switchToHelloView" text="Back" />
                    </buttons>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <padding>
                        <Insets right="10.0" />
                     </padding>
                  </ButtonBar>
               </children>
            </VBox>
            <StackPane fx:id="stackPane" prefHeight="720.0" prefWidth="960.0">
               <children>
                  <ImageView fx:id="skyImageView" fitHeight="480.0" fitWidth="640.0" pickOnBounds="true" preserveRatio="true" StackPane.alignment="CENTER" />
                  <ImageView fx:id="landImageView" fitHeight="480.0" fitWidth="640.0" pickOnBounds="true" preserveRatio="true" StackPane.alignment="CENTER">
                     <StackPane.margin>
                        <Insets top="100.0" />
                     </StackPane.margin>
                  </ImageView>
               </children>
            </StackPane>
         </children>
      </HBox>
   </children>
</AnchorPane>
